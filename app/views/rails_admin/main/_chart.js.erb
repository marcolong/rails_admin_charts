  $(function () {
    var chart;
    $(document).ready(function () {
      chart = new Highcharts.Chart({
        chart: {
          <% if @abstract_model.model.charts_type[index] == "column" %>
          type: 'column',
          <% end %>
          renderTo: "growth_rate_<%= index %>"
        },
        title: {
          text: '<%= @abstract_model.model.names[index] %>'
        },
        xAxis: {
          labels: {
            <% if @abstract_model.model.label_rotation == "-45" %>
            rotation: -45,
            <% elsif @abstract_model.model.label_rotation == "-90" %>
            rotation: -90,
            <% end %>
            align: 'right'
          },
          <% if @abstract_model.model.xaxis == "datetime" %>
          type: 'datetime',
          dateTimeLabelFormats: { // don't display the dummy year
            month: '%b',
            year: '%b'
          },
//          title: {
//            text: 'Date'
//          },
          <% elsif @abstract_model.model.chart_type == "pie" %>
          <% else %>
          categories: <%=raw @abstract_model.model.xaxis.to_json %>
          <% end %>

        },

//                yAxis: {
//                    title: {
//                        text: 'Counts'
//                    }
//                },
//        tooltip: {
//          headerFormat: '<b>{series.name}</b><br>',
////          pointFormat: '{point.x + 1:%e. %b}: {point.y:.2f} m'
//        },
        <%#tooltip: {%>
        <%#formatter: function() {%>
        <%#return ''+%>
        <%#Highcharts.dateFormat('%e. %b', this.x) +' -> '+ this.y;%>
        <%#}%>
        <%#},%>
        plotOptions: {
          line: {
            dataLabels: {
              enabled: true
            }
          },
          spline: {
            lineWidth: 4,
            states: {
              hover: {
                lineWidth: 5
              }
            },
            marker: {
              enabled: false,
              states: {
                hover: {
                  enabled: true,
                  symbol: 'circle',
                  radius: 5,
                  lineWidth: 1
                }
              }
            }
          }
        },
        <% if @abstract_model.model.charts_type[index] == "pie" %>
        series: [{
          type: 'pie',
          name: '<%= @abstract_model.model.names[index] %>',
          data: <%=raw @abstract_model.model.graphs_data[index].to_json %>
        }],
        <% else %>
        series: <%=raw @abstract_model.model.graphs_data[index].to_json %>,
        <% end %>
        navigation: {
          menuItemStyle: {
            fontSize: '10px'
          }
        }
      });
    });
  });
